/**
  ******************************************************************************
  * File Name          : pwm.c
  * Description        : This file provides code for controlling PWM.
  ******************************************************************************
  */

#include "pwm.h"

void PWM_Init() //Initialize PWM, timer 3, channel 1
{
	HAL_TIM_PWM_Start(&htim3, TIM_CHANNEL_1);
}

void PWM_SetDuty(float duty) //Set PWM duty
{
	TIM3 -> CCR1 = duty/100;
}

